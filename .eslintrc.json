{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
        "react/no-array-index-key": "off",
        "react/forbid-prop-types": "off",
        "array-callback-return": "off",
        "jsx-a11y/label-has-for": "off",
        "jsx-a11y/label-has-associated-control": "off"
    }
}